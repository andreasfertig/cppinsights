generator<unsigned int> fibonaccis()
{
  uint32_t a = 0;
  uint32_t b = 1;
  while(true) {
    co_yield b;
    std::tie(a, b).operator=(std::make_pair(b, a + b));
    co_yield a;
  }
  
}


