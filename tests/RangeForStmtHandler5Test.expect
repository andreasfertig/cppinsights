// https://mbevin.wordpress.com/2012/11/14/range-based-for/
class MyArrayWrapper
{
  int * data;
  int size;
  
  public: 
  inline int * begin()
  {
    return this->size > 0 ? &this->data[0] : nullptr;
  }
  
  inline int * end()
  {
    return this->size > 0 ? &this->data[this->size - 1] : nullptr;
  }
  
  // inline MyArrayWrapper() noexcept = default;
};




